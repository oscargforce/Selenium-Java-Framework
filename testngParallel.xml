<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite parallel="tests" name="Suite1" verbose="1">
    <listeners>
        <listener class-name="utilities.GlobalRetryAnnotater"/>
    </listeners>
    <test name="Regression1">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="selenium_tests.e2e"></class>
        </classes>
    </test>

    <test name="Regression2">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="selenium_tests.HomePageTests"></class>
        </classes>
    </test>
</suite>

